{
  "name": "sprinttool",
  "productName": "SprintTool",
  "version": "0.0.0",
  "main": "main.js",
  "license": "MIT",
  "description": "Sprint Tool app",
  "build": {
    "appId": "sprinttoolTMA",
    "asar": false,
    "directories": {
      "output": "winsetup"
    },
    "win": {
      "target": "nsis"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "electron .",
    "test": "ng serve",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build",
    "pack": "electron-builder --dir",
    "dist": "ng build && electron-builder && electron-builder install-app-deps",
    "electron": "ng build && electron .",
    "postinstall": "install-app-deps"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^0.4.5",
    "@angular/animations": "^5.2.9",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "bootstrap": "^4.0.0",
    "chart.js": "^2.7.2",
    "core-js": "^2.4.1",
    "electron-log": "^2.2.14",
    "font-awesome": "^4.7.0",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14",
    "pg": "^7.4.1"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/language-service": "^5.2.9",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "electron": "^1.8.3",
    "electron-packager": "^11.1.0",
    "electron-rebuild": "^1.7.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^2.6.4",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "jquery": "^3.3.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "pg": "^7.4.1",
    "popper.js": "^1.14.0",
    "primeng": "^5.2.3",
    "protractor": "~5.1.2",
    "rcedit": "^1.0.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
